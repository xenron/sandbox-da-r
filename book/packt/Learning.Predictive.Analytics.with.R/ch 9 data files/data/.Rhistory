lowerb = M - (1.96* (SD / sqrt(2000)))
upperb = M + (1.96* (SD / sqrt(2000)))
CIs[j,]= paste(round(lowerb,2), " - ", round(upperb,2))
}
names(CIs) = "95% C.I."
rownames(CIs) = names(ret)
